{
  "kind": "collectionType",
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "program_title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "program_description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "program_type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "private",
        "public"
      ],
      "required": true
    },
    "safe_harbour_type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "full",
        "partial"
      ],
      "required": true
    },
    "reward_type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "cash",
        "points"
      ],
      "required": true
    },
    "reward_range": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "json"
    },
    "program_guidelines_1": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "program_guidelines_2": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "program_scope": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true,
      "default": 0,
      "min": 0
    },
    "legal_terms": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "program_picture_url": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "is_closed": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "closed_at": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    },
    "reward_guidelines": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::submission.submission",
      "mappedBy": "program"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "programs"
    },
    "hackers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hacker.hacker",
      "mappedBy": "programs"
    },
    "company_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::company-user.company-user",
      "inversedBy": "programs"
    },
    "invitations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invitation.invitation",
      "mappedBy": "program"
    }
  }
}
